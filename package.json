{
  "name": "@mfsqlchecker/monorepo",
  "version": "0.0.1",
  "description": "Statically validate correctness of all your SQL queries. TypeScript, PostgreSQL",
  "scripts": {
    "build": "pnpm run --parallel --filter './packages/**' build",
    "dev": "pnpm run --parallel --filter './packages/**' dev",
    "test": "pnpm run --parallel --filter './packages/**' test",
    "typecheck": "pnpm run --parallel --filter './packages/**' typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MedFlyt/mfsqlchecker.git"
  },
  "keywords": [
    "TypeScript",
    "SQL",
    "PostgreSQL",
    "ORM"
  ],
  "author": "MedFlyt LLC.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MedFlyt/mfsqlchecker/issues"
  },
  "homepage": "https://github.com/MedFlyt/mfsqlchecker#readme",
  "workspaces": [
    "packages/*",
    "demo"
  ],
  "devDependencies": {
    "@tsconfig/node18": "2.0.1",
    "@types/node": "18.11.13",
    "tsx": "^3.12.7",
    "typescript": "5.0.4"
  }
}
