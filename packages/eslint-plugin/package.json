{
  "name": "@medflyt/eslint-plugin-sql-checker",
  "version": "0.0.4",
  "private": false,
  "description": "An ESLint plugin for validating the correctness of SQL queries",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "typecheck": "tsc",
    "build": "tsup --minify",
    "test": "mocha -r=@esbuild-kit/cjs-loader '**/*.test.ts' --timeout 10000",
    "dev": "tsup --watch='../@(eslint-plugin|core|shared)/**/*.ts'"
  },
  "author": "MedFlyt LLC.",
  "license": "MIT",
  "homepage": "https://github.com/MedFlyt/mfsqlchecker#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "chalk": "4.1.2",
    "fp-ts": "2.15.0",
    "postgres": "3.3.4",
    "source-map-support": "0.5.13",
    "stack-trace": "0.0.10",
    "tiny-invariant": "1.3.1",
    "zod": "3.21.4",
    "zod-to-json-schema": "3.21.1"
  },
  "peerDependencies": {
    "embedded-postgres": "*"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "2.4.2",
    "@mfsqlchecker/client": "workspace:*",
    "@mfsqlchecker/core": "workspace:*",
    "@mfsqlchecker/shared": "workspace:*",
    "@types/eslint": "^8.37.0",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.13",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@typescript-eslint/typescript-estree": "^5.59.2",
    "@typescript-eslint/utils": "^5.59.2",
    "embedded-postgres": "15.3.0-beta.8",
    "esbuild": "^0.17.18",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.40.0",
    "eslint-plugin-local-rules": "^1.3.2",
    "mocha": "10.2.0",
    "synckit": "^0.8.5",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "5.0.4"
  }
}
